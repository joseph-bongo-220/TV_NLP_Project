{
    "app":{
        "base_url": "https://genius.com/albums/",
        "cutoff": 1000,
        "ngrams": [1,2],
        "take_top": 50,
        "num_characters": 12,
        "measure": "pagerank",
        "pickle": true,
        "json": false,
        "shows": ["Game of thrones", "The office us"],
        "min_fuzzy_matching_ratio": 90,
        "min_partial_fuzzy_matching_ratio": 100
    },
    "Game of thrones":{
        "extension": "Season-{}-scripts",
        "seasons": 7,
        "pickle_path": "GOT_Pickle.pkl",
        "character_config": "GOT_Characters.json"
    },
    "The office us":{
        "extension": "Season-{}",
        "seasons": 2,
        "pickle_path": "Office_Pickle_2.pkl",
        "charcter_config": "Office_Characters.json"
    }
}