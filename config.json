{
    "app":{
        "debug": true,
        "base_url": "https://genius.com/albums/",
        "position_cutoff": 5000,
        "graph_cutoff": 500,
        "ngrams": [1,2],
        "take_top": 50,
        "measure": "pagerank",
        "pickle": true,
        "json": false,
        "shows": ["Game of thrones", "The office us"],
        "min_fuzzy_matching_ratio": 90,
        "min_partial_fuzzy_matching_ratio": 100,
        "DAN_sentence_encoder_url": "https://tfhub.dev/google/universal-sentence-encoder/2"
    },
    "Game of thrones":{
        "extension": "Season-{}-scripts",
        "seasons": 7,
        "pickle_path": "GOT_Pickle.pkl",
        "character_config": "GOT_Characters.json",
        "num_characters": 50,
        "remove_chars": ["", "MAN"],
        "sentence_pkl_path": "GOT_sentence_embeddings2.pkl",
        "doc_pkl_path": "GOT_doc_embeddings.pkl"
    },
    "The office us":{
        "extension": "Season-{}",
        "seasons": 2,
        "pickle_path": "Office_Pickle_2.pkl",
        "charcter_config": "The_Office_Characters.json",
        "num_characters": 20,
        "remove_chars": ["", "MAN"],
        "sentence_pkl_path": "office_sentence_embeddings.pkl",
        "doc_pkl_path": "office_doc_embeddings.pkl"
    }
}